{
    "manifest_version": 3,
    "name": "Clicky",
    "description": "For the clicky keyboard enjoyers.",
    "version": "1.1",
    "icons":
    {
        "512": "icon_512.png"
    },
    "author": "Opera Software",
    "developer":
    {
        "name": "Opera Software"
    },
    "mod":
    {
        "schema_version": 1,
        "payload":
        {
        
            "keyboard_sounds":
            {
                "TYPING_LETTER":
                [
                    "sounds/clicky1.wav",
                    "sounds/clicky4.wav",
                    "sounds/clicky2.wav",
                    "sounds/clicky3.wav"

                ],
                "TYPING_ENTER":
                [
                    "sounds/clickyenter.wav"
                ],
                "TYPING_SPACE":
                [
                    "sounds/clickyspace.wav"
                ],
                "TYPING_BACKSPACE":
                [
                    "sounds/clickybackspace.wav"
                ]
            }
       }
    }
    
}